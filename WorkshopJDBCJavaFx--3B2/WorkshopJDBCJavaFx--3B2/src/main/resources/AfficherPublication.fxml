<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AfficherPublicationController">
    <children>
        <TableView fx:id="tvPublications" layoutX="14.0" layoutY="14.0" prefHeight="329.0" prefWidth="573.0">
            <columns>
                <TableColumn fx:id="colTitre" prefWidth="131.0" text="Titre" />
                <TableColumn fx:id="colContenu" prefWidth="272.0" text="Contenu" />
                <TableColumn fx:id="colDate" prefWidth="96.0" text="Date" />
                <TableColumn fx:id="colUser" prefWidth="73.0" text="User ID" />
            </columns>
        </TableView>
        <Button layoutX="14.0" layoutY="360.0" mnemonicParsing="false" onAction="#AjouterPublication" text="Ajouter Publication" />
        <Button layoutX="161.0" layoutY="360.0" mnemonicParsing="false" onAction="#ModifierPublication" text="Modifier Publication" />
        <Button layoutX="311.0" layoutY="360.0" mnemonicParsing="false" onAction="#SupprimerPublication" text="Supprimer Publication" />
        <Button layoutX="467.0" layoutY="360.0" mnemonicParsing="false" onAction="#AfficherCommentaires" text="Voir Commentaires" />
    </children>
</AnchorPane>
